{
  "name": "pecunia",
  "version": "1.0.0",
  "description": "A Monzo app",
  "scripts": {
    "start": "node_modules/.bin/ts-node -r tsconfig-paths/register bin/www",
    "tunnel": "node_modules/.bin/ts-node src/tunnel",
    "lint": "node_modules/.bin/tslint --project . 'src/**/*.ts'",
    "lint-fix": "node_modules/.bin/tslint --fix --project . 'src/**/*.ts'",
    "test": "./node_modules/mocha/bin/mocha -r ts-node/register -r tsconfig-paths/register 'src/**/*.spec.ts'",
    "build": "npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pads/pecunia.git"
  },
  "keywords": [
    "monzo"
  ],
  "author": "pads",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pads/pecunia/issues"
  },
  "homepage": "https://github.com/pads/pecunia#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "connect-memcached": "^0.2.0",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "urijs": "^1.19.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^4.0.5",
    "@types/chai-as-promised": "^7.1.0",
