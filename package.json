    "@types/errorhandler": "0.0.32",
    "@types/es6-promise": "0.0.33",
    "@types/express": "^4.0.39",
    "@types/express-session": "^1.15.6",
    "@types/mocha": "^2.2.44",
    "@types/nock": "^8.2.1",
    "@types/request": "^2.0.8",
    "@types/request-promise-native": "^1.0.10",
    "@types/sinon": "^4.0.0",
    "@types/sinon-chai": "^2.7.29",
    "@types/urijs": "^1.15.34",
    "@types/uuid": "^3.4.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "mocha": "^4.0.1",
    "ngrok": "^2.2.23",
